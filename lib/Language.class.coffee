Size = require './Size.class'

class Language
	constructor: (@code) ->
		@size4 = new Size this, 4
		@size5 = new Size this, 5

		unless @code in ['de']
			throw "This language is not supported at the moment"

	getSize: (size) =>
		switch size
			when 4 then return @size4
			when 5 then return @size5 
			else throw "Wrong size, needs to be 4 or 5"

	getLetterDistributionString: () =>
		if @code == "de"
			return ""+
				""+
				""

module.exports = Language